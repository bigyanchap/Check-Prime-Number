public class IsPrime {
	public static void main(String args[]){
		System.out.println(isPrime(17));
	}
	static boolean isPrime(int n) {
		if(n==1)return false;
		boolean prime=true;
	    for(int i=2;i<n;i++) {
	        if((n%i)==0){
	        	prime=false;
		        break;
	        }
	    }
	    return prime;
	}
}
